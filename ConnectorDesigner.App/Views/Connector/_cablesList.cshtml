   <!-- Modal -->
<div class="modal fade" id="cableModal" role="dialog">
    <div class="modal-dialog" style="width: 1100px;">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" style="text-align:center;">select cable</h4>
            </div>
            <div class="modal-body" style="height: 500px; overflow: auto; font-size: 11px;">
                <table id="cableList" cellspacing="0" class="display">

                    <thead>
                        <tr>
                            <th>Select</th>
                            <th>Description</th>
                            <th>Hubber + Shuhner Type</th>
                            <th>Impediment</th>
                            <th>Operational Freq.</th>
                            <th>Jacker Diameter</th>
                            <th>Jacket Material</th>
                            <th>Center conductor material</th>
                            <th>Center conductor design</th>
                            <th>Attenuation</th>
                            <th> Product details</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model.Cables)
                        {
                            ConnectorDesigner.App.Models.File[] images = new ConnectorDesigner.App.Models.File[10];
                            @*@item.Images.CopyTo(images);*@
                            <tr>
                                <td><input name="@item.Id" onchange="connector.manageRadio(@item.Id);" type="radio" /></td>
                                <td>@item.Description</td>
                                <td>@item.Type</td>
                                <td>@item.Impediments</td>
                                <td>@item.OperationFreq</td>
                                <td>@item.JackerDiameter</td>
                                <td>@item.JacketMaterial</td>
                                <td>@item.CCM</td>
                                <td>@item.CCD</td>
                                <td>@item.Attenuation</td>
                                <td><a href="@item.ProductDetails" target="_blank"><img src="/content/image/ie.gif" /></a></td>
                            </tr>
                        }
                    </tbody>

                </table>
            </div>
            <div class="modal-footer">
                <button type="button" id="btnApply" class="btn btn-success" disabled="true" onclick="connector.addWireInCanvas();">Apply</button>
            </div>
        </div>

    </div>
</div>
<!-- End modal -->

