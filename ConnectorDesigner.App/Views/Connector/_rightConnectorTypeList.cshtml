   <!-- Modal -->
<div class="modal fade" id="rightConnectorTypeModal" role="dialog">
    <div class="modal-dialog" style="width: 800px;">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" style="text-align:center;">select right end connector type</h4>
            </div>
            <div class="modal-body" style="height: 400px; overflow: auto;font-size: 12px; ">
                <table id="connectorTypeList" cellspacing="0" class="display">

                    <thead>
                        <tr>
                            <th>Select  </th>
                            <th>Hubber + Shuhner Type</th>
                            <th>Impediment  </th>
                            <th>Interface Freq.</th>
                            <th>Outer contact plating</th>
                            <th>Cable Entry outer contact</th>
                            <th>Flange Type</th>
                            <th>Product details</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model.RightTypes)
                        {
                            <tr>
                                <td><input name="@item.Id" onchange="connector.manageRightTypeRadio(@item.Id);" type="radio" /></td>
                                <td>@item.Type</td>
                                <td>@item.Impediment</td>
                                <td>@item.InterfaceFreq</td>
                                <td>@item.OCP</td>
                                <td>@item.CEOC</td>
                                <td>@item.FlangType</td>
                                <td><a href="@item.ProductDetails" target="_blank"><img src="/content/image/ie.gif" /></a></td>
                            </tr>
                        }
                    </tbody>

                </table>
            </div>
            <div class="modal-footer">
                <button type="button" id="btnRightConnectorTypeApply" class="btn btn-success" disabled="true" onclick="connector.addRightTypeImage();">Apply</button>
            </div>
        </div>

    </div>
</div>
<!-- End modal -->

